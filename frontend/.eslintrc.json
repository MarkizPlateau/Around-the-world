{
  "extends": "next/core-web-vitals",
  "plugins": ["react", "@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-sort-props": [
      "warn",
      {
        "ignoreCase": true,
        "callbacksLast": true,
        "shorthandFirst": true
      }
    ]
  }
}
